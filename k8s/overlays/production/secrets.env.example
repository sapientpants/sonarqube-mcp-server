# Copy this file to secrets.env and fill in the actual values
# DO NOT commit secrets.env to version control

# Default SonarQube token
SONARQUBE_TOKEN=squ_production_token_here

# Service Account Tokens
SERVICE_ACCOUNT_SA1_USERNAME=sa-team-alpha
SERVICE_ACCOUNT_SA1_TOKEN=squ_production_team_alpha_token
SERVICE_ACCOUNT_SA1_GROUP_PATTERN=team-alpha|alpha-.*

SERVICE_ACCOUNT_SA2_USERNAME=sa-team-beta
SERVICE_ACCOUNT_SA2_TOKEN=squ_production_team_beta_token
SERVICE_ACCOUNT_SA2_GROUP_PATTERN=team-beta|beta-.*

SERVICE_ACCOUNT_SA3_USERNAME=sa-team-security
SERVICE_ACCOUNT_SA3_TOKEN=squ_production_team_security_token
SERVICE_ACCOUNT_SA3_GROUP_PATTERN=security|sec-.*

SERVICE_ACCOUNT_SA4_USERNAME=sa-team-devops
SERVICE_ACCOUNT_SA4_TOKEN=squ_production_team_devops_token
SERVICE_ACCOUNT_SA4_GROUP_PATTERN=devops|ops-.*

SERVICE_ACCOUNT_SA5_USERNAME=sa-contractors
SERVICE_ACCOUNT_SA5_TOKEN=squ_production_contractors_token
SERVICE_ACCOUNT_SA5_GROUP_PATTERN=contractors|external-.*